{
  "name": "skynet",
  "version": "3.0.0",
  "author": "Vakot",
  "license": "MIT",
  "description": "All-purpose Discord bot with highly customisible interface. Basically the bot is just a framework for creating simple and isolated event listeners and responses. Every user can create actions for different events, such as slash-commands, message-commands, voice events, buttons, menus and so on. Commands, buttons and menus also can be registered directly from WEB-application.",
  "keywords": [
    "bot",
    "discord"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "next dev",
    "dev:server": "nodemon bot/index",
    "build": "npm-run-all --parallel build:*",
    "build:next": "next build",
    "start": "npm-run-all --parallel start:*",
    "start:next": "next start",
    "start:server": "set NODE_ENV=production && ts-node bot/index"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "antd": "^5.16.2",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "discord.js": "^14.14.1",
    "emoji-picker-react": "^4.9.2",
    "mongoose": "^8.2.0",
    "next": "14.2.2",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "express": "^4.19.2",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "react-redux": "^9.1.1",
    "sass": "^1.75.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tspath": "^2.6.8",
    "typescript": "^5"
  }
}
